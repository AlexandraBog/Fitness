<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Fitness">
  
    <FontFamily x:Key="PrimaryFont">/Fonts/#Alexandra Zeferino Two</FontFamily>

    <Style x:Key="ButtonStyle" x:Name="ButtonStyle">
        <Setter Property="Control.Background" Value="#E56E5759"/>
        <Setter Property="Control.Foreground" Value="White"/>
    </Style>

    <SolidColorBrush x:Key="ListBoxBack" Color="#CC6E5757"/>

</ResourceDictionary>